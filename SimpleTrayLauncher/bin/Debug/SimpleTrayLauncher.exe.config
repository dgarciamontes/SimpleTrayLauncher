<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="ScriptSettings" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
  </configSections>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>


  <ScriptSettings>
    <!--You can add all items as you want as follow:
    <add key="sxx"    value="batfile.bat" /> 
    <add key="nxx"    value="NameToShow" />
    <add key="axx"    value="" /> Only if we want declare an absolute path-->


    <add key="s01" value="HelloWorld.bat"/>
    <add key="n01" value="HELLO"/>
    
    <add key="s02" value="notepad.exe"/>
    <add key="n02" value="NOTEPAD"/>
    <add key="a02" value=""/>


    <add key="DoubleClickScript" value="calc.exe"/>
    <add key="aDoubleClickScript" value=""/>
    
    <add key="ScriptFolder" value="Script"/>
    <add key="IconName" value="netfol.ico"/>
    
  </ScriptSettings>
  
  
</configuration>
